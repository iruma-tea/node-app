<!-- リスト２ー１４ -->
<!DOCTYPE html>
<html lang="ja">
<head>
    <meta charset="UTF-8">
    <title><%=title %></title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.css" crossorigin="anonymous">
</head>
<body class="container">
    <header>
        <h1 class="display-4"><%=title %></h1>
    </header>
    <div role="main">
        <p><%=content %></p>
        <!-- リスト３－１２ -->
        <% let id = 0; %>
        <% for (let key in data) { %>
            <%- include('data_item', {id:++id, key:key, val:[data[key]]}) %>
        <% } %>
    </div>
</body>
</html>